# Connect Dots Multi-Hop SQL Query Generation Configuration
# Copy this file to .env and fill in your values

# Required: OpenRouter API key for LLM access
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: OpenAI API base URL (defaults to OpenRouter)
OPENAI_API_BASE=https://openrouter.ai/api/v1

# Optional: Path to BIRD database directory
# Default: ../bird/train/train_databases/train_databases
BIRD_DB_PATH=../bird/train/train_databases/train_databases

# Optional: Default model to use
# Default: qwen/qwen-2.5-72b-instruct
DEFAULT_MODEL=qwen/qwen-2.5-72b-instruct

# Optional: Rate limiting (requests per minute)
# Default: 60
REQUESTS_PER_MINUTE=60 